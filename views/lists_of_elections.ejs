<p class="text-red-600">
    <%= (elections_list.length==0)?"Empty Elections" : "" %>
</p>
<ul class="list-none">
    <% for (let i=0; i<elections_list.length; i++) { %>
        <li>
            <div class="flex items-center w-fit my-2 px-2 py-1 rounded">
                <p class="m-1 text-sm text-black-300 ">
                    <a href="/listofelections/<%=elections_list[i].id %>" class="m-1 text-m text-blue-600 ">
                        <%=i+1%>.  <%= elections_list[i].electionName %> 
                    </a>
                </p>
            </div>
        </li>
    <% } %>
  </ul>
